   <children>
      <Label layoutX="63.0" layoutY="53.0" text="Model" />
      <TextField fx:id="modelText" layoutX="114.0" layoutY="49.0" />
      <ChoiceBox fx:id="typeChoice" layoutX="113.0" layoutY="99.0" prefWidth="150.0" />
      <TextField fx:id="yearText" layoutX="114.0" layoutY="149.0" />
      <Label layoutX="63.0" layoutY="103.0" text="Type" />
      <Label layoutX="63.0" layoutY="153.0" text="Year" />
      <Button fx:id="register" layoutX="146.0" layoutY="200.0" mnemonicParsing="false" onAction="#showInfo" text="Register bike" />
      <Button fx:id="back" layoutX="146.0" layoutY="200.0" mnemonicParsing="false" onAction="#toMenu" text="Back" />
      <Label fx:id="infoLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="114.0" layoutY="257.0" prefHeight="76.0" prefWidth="149.0" text="Label" />
   </children>
</AnchorPane>
